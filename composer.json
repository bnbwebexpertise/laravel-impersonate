{
  "name": "bnbwebexpertise/laravel-impersonate",
  "description": "Laravel Impersonate is a plugin that allows to you to authenticate as your users forked from 404labfr/laravel-impersonate.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "plugin",
    "impersonate",
    "impersonation",
    "user",
    "auth",
    "laravel-package",
    "laravel-plugin"
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": "5.8.* | ^6.0 | ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 | ^8.0",
    "orchestra/testbench": "^4.0 | ^5.0",
    "orchestra/database": "^4.0 | ^5.0",
    "symfony/mime": ">=4.3.8",
    "symfony/http-foundation": ">=4.3.8"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "404lab",
      "email": "web@404lab.fr"
    },
    {
      "name": "Jérémy Gaulin",
      "email": "jeremy@bnb.re"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bnb\\Laravel\\Impersonate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bnb\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bnb\\Laravel\\Impersonate\\ImpersonateServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
